  <style>
		body {font-family: Arial, Helvetica, sans-serif;}
		.stick-bar {
		  position: fixed;
		  top: 1%;
		  width: 100%;
		}
		
		
		
		
		.icon-bar {
		  position: fixed;
		  top: 50%;
		  -webkit-transform: translateY(-50%);
		  -ms-transform: translateY(-50%);
		  transform: translateY(-50%);
		}
		
		.icon-bar a {
		  display: block;
		  text-align: center;
		  padding: 16px;
		  transition: all 0.3s ease;
		  color: white;
		  font-size: 20px;
		}
		
		.icon-bar a:hover {
		  background-color: #007bb5;
		}
		
		.facebook {
		  background: #3B5998;
		  color: white;
		}
		
		.twitter {
		  background-color: gray ;
		  color: red;
		}
		
		.google {
		  background: #dd4b39;
		  color: white;
		}
		
		.linkedin {
		  background: #00dfe2;
		  color: white;
		}
		
		.youtube {
		  background: #345;
		  color: white;
		}
		
		.content {
		  margin-left: 75px;
		  font-size: 30px;
		}
		
		
		
		.active {
		  background-color: blue;
		}
		
		
		.mySlides {display: none;}
		img {vertical-align: middle;}
		
		/* Slideshow container */
		.slideshow-container {
		  max-width: 1000px;
		  position: relative;
		  margin: auto;
		}
		
		/* Caption text */
		.text {
		  color: #f2f2f2;
		  font-size: 15px;
		  padding: 8px 12px;
		  position: absolute;
		  bottom: 8px;
		  width: 100%;
		  text-align: center;
		}
		
		/* Number text (1/3 etc) */
		.numbertext {
		  color: #f2f2f2;
		  font-size: 12px;
		  padding: 8px 12px;
		  position: absolute;
		  top: 0;
		}
		
		/* The dots/bullets/indicators */
		.dot {
		  height: 15px;
		  width: 15px;
		  margin: 0 2px;
		  background-color: #bbb;
		  border-radius: 50%;
		  display: inline-block;
		  transition: background-color 0.6s ease;
		}
		
		
		}
		
		/* Fading animation */
		.fade {
		  -webkit-animation-name: fade;
		  -webkit-animation-duration: 1.5s;
		  animation-name: fade;
		  animation-duration: 1.5s;
		}
		
		@-webkit-keyframes fade {
		  from {opacity: .4}
		  to {opacity: 1}
		}
		
		@keyframes fade {
		  from {opacity: .4}
		  to {opacity: 1}
		}
		
		/* On smaller screens, decrease text size */
		@media only screen and (max-width: 300px) {
		  .text {font-size: 11px}
		}
		</style>

